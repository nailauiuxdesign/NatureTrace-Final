# Environment Variables for NatureTrace
# Copy this file to .env and fill in your actual values

# FreeSound API Key (get from https://freesound.org/help/developers/)
FREESOUND_API_KEY=your_freesound_api_key_here

# Groq API Key (get from https://groq.com/)
GROQ_API_KEY=your_groq_api_key_here

# Azure Computer Vision (optional)
AZURE_COMPUTER_VISION_ENDPOINT=your_azure_endpoint_here
AZURE_COMPUTER_VISION_KEY=your_azure_key_here

# Snowflake Database (optional - for data persistence)
SNOWFLAKE_ACCOUNT=your_snowflake_account
SNOWFLAKE_USER=your_snowflake_user
SNOWFLAKE_PASSWORD=your_snowflake_password
SNOWFLAKE_WAREHOUSE=your_warehouse
SNOWFLAKE_DATABASE=your_database
SNOWFLAKE_SCHEMA=your_schema
